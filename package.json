{
  "name": "digital-cart-flow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "node server.js",
    "lint": "eslint src --max-warnings=0",
    "typecheck": "/bin/sh -c tsc --project tsconfig.json --noEmit",
    "prettier": "prettier --loglevel silent --write .",
    "test": "jest --maxWorkers=100%"
  },
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">=1.22.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn prettier",
      "yarn typecheck",
      "yarn lint --fix",
      "yarn test --bail --silent --passWithNoTests"
    ]
  },
  "dependencies": {
    "@hookform/devtools": "4.0.2",
    "axios": "^1.4.0",
    "credit-card-type": "^9.1.0",
    "date-fns": "^2.30.0",
    "format-string-by-pattern": "^1.2.2",
    "next": "13.4.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.39.7",
    "react-icons": "^4.10.0",
    "styled-components": "5.3.5"
  },
  "devDependencies": {
    "@babel/core": "7.22.5",
    "@babel/preset-env": "7.22.5",
    "@babel/preset-react": "7.22.5",
    "@babel/preset-typescript": "7.22.5",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "7.0.1",
    "@types/jest": "29.5.2",
    "@types/node": "20.3.1",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "babel-jest": "29.5.0",
    "babel-plugin-inline-react-svg": "^2.0.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-styled-components": "1.13.2",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.6",
    "eslint-plugin-import-helpers": "1.3.1",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "jest-styled-components": "7.1.1",
    "lint-staged": "13.0.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "tsc": "2.0.4",
    "typescript": "^4.3.5"
  }
}
